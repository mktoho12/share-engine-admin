{
  "name": "share-engine-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api:update-submodule": "git submodule update --init --remote",
    "api:gen-schema": "docker compose -f submodules/share-engine/backend/docker-compose.yml run api python app/generate-openapi.py",
    "api:gen-client": "rm -rf src/api/{model,api.ts} && orval",
    "api:update": "bun api:update-submodule && bun api:gen-schema && bun api:gen-client",
    "api:serve": "ORIGIN=http://localhost:3000 docker compose -f submodules/share-engine/backend/docker-compose.yml up",
    "api:serve:stop": "docker compose -f submodules/share-engine/backend/docker-compose.yml down"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.21.7",
    "@tanstack/react-query-devtools": "^5.21.7",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "js-cookie": "^3.0.5",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "openapi3-ts": "^4.2.2",
    "orval": "^6.25.0",
    "postcss": "^8",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}